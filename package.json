{
  "name": "jarvis-v4",
  "version": "4.0.0",
  "description": "Multi-Agent AI Assistant with 29 Specialized Agents",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:compliance": "jest --testPathPattern=tests/compliance",
    "test:smoke": "jest --testPathPattern=tests/smoke --runInBand --detectOpenHandles --forceExit",
    "test:smoke:watch": "jest --testPathPattern=tests/smoke --watch",
    "test:phase1": "jest tests/smoke/phase1-backend.test.ts",
    "test:phase2": "jest tests/smoke/phase2-socketio.test.ts",
    "test:phase3": "jest tests/smoke/phase3-errors.test.ts",
    "test:all": "npm run type-check && npm run lint && npm test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "backup:create": "tsx src/scripts/create-backup.ts",
    "backup:list": "tsx src/scripts/list-backups.ts",
    "backup:cleanup": "tsx src/scripts/cleanup-backups.ts",
    "r-tuning:generate": "tsx src/scripts/generate-r-tuning-dataset.ts",
    "r-tuning:validate": "tsx src/scripts/validate-r-tuning-dataset.ts",
    "r-tuning:export": "tsx src/scripts/export-r-tuning-dataset.ts",
    "playwright:install": "playwright install chromium",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "prepare": "husky install"
  },
  "keywords": [
    "ai",
    "assistant",
    "multi-agent",
    "typescript",
    "jarvis"
  ],
  "author": "YorkieB",
  "license": "MIT",
  "dependencies": {
    "@deepgram/sdk": "^4.11.3",
    "@elevenlabs/elevenlabs-js": "^2.29.0",
    "@google-cloud/speech": "^7.2.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@sentry/node": "^7.99.0",
    "@sentry/profiling-node": "^7.99.0",
    "dotenv": "^16.6.1",
    "e2b": "^2.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^128.0.0",
    "node-rtsp-stream": "^0.0.9",
    "onnxruntime-node": "^1.20.0",
    "onvif": "^0.6.6",
    "openai": "^4.104.0",
    "pg": "^8.17.1",
    "playwright": "^1.40.0",
    "pm2": "^6.0.14",
    "socket.io": "^4.8.3",
    "vscode-languageserver-protocol": "^3.17.5",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prisma": "^7.2.0",
    "socket.io-client": "^4.8.3",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
